                         SalesItems.SaleID = Sales.SaleID
GROUP BY Clients.Name, derivedtbl_1.Gained, derivedtbl_1.ClientID, derivedtbl_1.Date, derivedtbl_1.BillNO
HAVING        (Clients.Name = @ClientName)</CommandText>
