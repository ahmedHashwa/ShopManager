      <Query>
        <DataSourceName>DatabaseConnectionString</DataSourceName>
        <CommandText>
          SELECT        Purchases.PurchaseID AS ID, Purchases.Date, Purchases.N_Cages, Purchases.N_Kilos, Purchases.Paid, Purchases.BillNO, Suppliers.Name AS اسم_المورد,
          RawKinds.Name AS اسم_الخام
          FROM            Purchases INNER JOIN
          RawKinds ON Purchases.RawID = RawKinds.RawID INNER JOIN
          Suppliers ON Purchases.SupplierID = Suppliers.SupplierID
        </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
