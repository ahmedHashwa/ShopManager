  <DataSets>
    <DataSet Name="ReportDataSet_إجمالي_مبيعات_كمية_العصير">
      <Fields>
        <Field Name="اسم_العصير">
          <DataField>اسم العصير</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="نوع_العصير">
          <DataField>نوع العصير</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="الكمية">
          <DataField>الكمية</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="القيمة">
          <DataField>القيمة</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DatabaseConnectionString</DataSourceName>
        <CommandText>SELECT        SalesKinds.Name AS [اسم العصير], SalesSubKinds.Name AS [نوع العصير], SUM(derivedtbl_1.Amount) AS الكمية, SUM(derivedtbl_1.Amount * derivedtbl_1.PiecePrice) 
                         AS القيمة
FROM            (SELECT        SalesItems.Amount, SalesItems.PiecePrice, SalesItems.SalesKindID, SalesItems.SalesSubKindID
                           FROM            SalesItems INNER JOIN
                                                    [Sales] ON SalesItems.SaleID = [Sales].SaleID
                           WHERE        ([Sales].SellDate BETWEEN @StartDate AND @EndDate)) AS derivedtbl_1 INNER JOIN
                         SalesSubKinds ON derivedtbl_1.SalesKindID = SalesSubKinds.SalesKindID AND derivedtbl_1.SalesSubKindID = SalesSubKinds.SalesSubKindID INNER JOIN
                         SalesKinds ON SalesSubKinds.SalesKindID = SalesKinds.SalesKindID
GROUP BY SalesKinds.Name, SalesSubKinds.Name
ORDER BY [اسم العصير], [نوع العصير]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
