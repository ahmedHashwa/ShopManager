  <DataSets>
    <DataSet Name="ReportDataSet_إجمالي_مصروفات">
      <Fields>
        <Field Name="نوع_المصروف">
          <DataField>نوع المصروف</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="القيمة">
          <DataField>القيمة</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DatabaseConnectionString</DataSourceName>
        <CommandText>SELECT        [Expenses Types].Description AS [نوع المصروف], SUM(Table1.Amount) AS القيمة
FROM            (SELECT        Amount, ExpenseTypeID
                           FROM            Expenses
                           WHERE        (Date BETWEEN @StartDate AND @EndDate)) AS Table1 INNER JOIN
                         [Expenses Types] ON Table1.ExpenseTypeID = [Expenses Types].ExpenseTypeID
GROUP BY [Expenses Types].Description</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <rd:DataSetInfo>
        <rd:DataSetName>ReportDataSet</rd:DataSetName>
        <rd:TableName>إجمالي مصروفات</rd:TableName>
        <rd:TableAdapterFillMethod>Fill</rd:TableAdapterFillMethod>
