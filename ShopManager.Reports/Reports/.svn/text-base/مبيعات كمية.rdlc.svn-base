        </Field>
      </Fields>
      <Query>
        <DataSourceName>DatabaseConnectionString</DataSourceName>
        <CommandText>
          SELECT        [Sales].SellDate AS التاريخ, SalesKinds.Name AS [نوع العصير], SalesSubKinds.Name AS [عدد اللترات], [Sales].Gained AS [وارد فلوس], SalesItems.Amount AS الكمية,
          Clients.Name AS [اسم العميل], SalesItems.PiecePrice * SalesItems.Amount AS منصرف, [Sales].BillNO AS [رقم الفاتورة]
          FROM            SalesKinds INNER JOIN
          SalesSubKinds ON SalesKinds.ID = SalesSubKinds.KindID INNER JOIN
          SalesItems ON SalesSubKinds.ID = SalesItems.SalesSubKindID INNER JOIN
          [Sales] ON SalesItems.SalesID = [Sales].ID INNER JOIN
          Clients ON [Sales].ClientID = Clients.ID
          WHERE        (Clients.Name = @Clname)
        </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!التاريخ.Value</FilterExpression>
          <Operator>Between</Operator>
          <FilterValues>
            <FilterValue>=Parameters!StartDate.Value</FilterValue>
            <FilterValue>=Parameters!EndDate.Value</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
