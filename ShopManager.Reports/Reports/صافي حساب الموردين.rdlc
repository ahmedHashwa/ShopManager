  <DataSets>
    <DataSet Name="ReportDataSet_صافي_حساب_الموردين">
      <Fields>
        <Field Name="اسم_المورد">
          <DataField>اسم المورد</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="رصيد_أول_المدة">
          <DataField>رصيد أول المدة</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DatabaseConnectionString</DataSourceName>
        <CommandText>SELECT        BeginningBalance AS [رصيد أول المدة], Name AS [اسم المورد]
FROM            (SELECT        Suppliers.Name, CASE WHEN SUM(Purchases_1.KiloPrice * Purchases_1.N_Kilos - Purchases_1.Paid) IS NOT NULL 
                                                    THEN SUM(Purchases_1.KiloPrice * Purchases_1.N_Kilos - Purchases_1.Paid) 
                                                    + Suppliers.BeginningBalance ELSE Suppliers.BeginningBalance END AS BeginningBalance
                           FROM            Suppliers INNER JOIN
                                                    Purchases AS Purchases_1 ON Suppliers.SupplierID = Purchases_1.SupplierID
                           GROUP BY Suppliers.SupplierID, Suppliers.BeginningBalance, Suppliers.Name) AS SuppliersCredits</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <rd:DataSetInfo>
        <rd:DataSetName>ReportDataSet</rd:DataSetName>
        <rd:TableName>صافي حساب الموردين</rd:TableName>
        <rd:TableAdapterFillMethod>Fill</rd:TableAdapterFillMethod>
