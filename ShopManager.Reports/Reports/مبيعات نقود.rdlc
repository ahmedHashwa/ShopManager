  <DataSets>
    <DataSet Name="DataSet_مبيعات_نقود">
      <Fields>
        <Field Name="الاسم">
          <DataField>الاسم</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="منصرف">
          <DataField>منصرف</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="وارد">
          <DataField>وارد</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="التاريخ">
          <DataField>التاريخ</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="رقم_الفاتورة">
          <DataField>رقم_الفاتورة</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DatabaseConnectionString</DataSourceName>
        <CommandText>SELECT        Clients.Name AS الاسم, SUM(SalesItems.Amount * SalesItems.PiecePrice) AS منصرف, derivedtbl_1.Gained AS وارد, [Sales].SellDate AS التاريخ
FROM            Clients INNER JOIN
                         [Sales] ON Clients.ID = [Sales].ClientID INNER JOIN
                         SalesItems ON [Sales].ID = SalesItems.SalesID INNER JOIN
                             (SELECT        SUM(Gained) AS Gained, ClientID
                                FROM            [Sales] AS [Sales _1]
                                GROUP BY ClientID) AS derivedtbl_1 ON [Sales].ClientID = derivedtbl_1.ClientID
GROUP BY Clients.Name, derivedtbl_1.Gained, [Sales].SellDate
HAVING        (Clients.Name = @ClientName)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!التاريخ.Value</FilterExpression>
          <Operator>Between</Operator>
          <FilterValues>
            <FilterValue>=Parameters!StartDate.Value</FilterValue>
            <FilterValue>=Parameters!EndDate.Value</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
