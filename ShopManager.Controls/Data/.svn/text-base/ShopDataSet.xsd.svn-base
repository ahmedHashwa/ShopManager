                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        Purchases.PurchaseID AS مسلسل, Purchases.BillNO AS [رقم الفاتورة], Suppliers.Name AS [اسم المورد], Purchases.Paid AS المدفوع, 
                         Purchases.CagePrice * Purchases.N_Cages + Purchases.KiloPrice * Purchases.N_Kilos AS القيمة, RawKinds.Name AS [اسم الخام], Purchases.N_Cages AS [الكمية بالأقفاص], 
                         Purchases.CagePrice AS البيعه, Purchases.KiloPrice AS [سعر الكيلو], Purchases.N_Kilos AS [الكمية بالكيلو], Purchases.Date AS [تاريخ الشراء]
